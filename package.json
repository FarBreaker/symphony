{
	"name": "symphony",
	"version": "0.1.0",
	"bin": {
		"symphony": "bin/symphony.js"
	},
	"type": "module",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk",
		"docs": "npx typedoc",
		"deploy": "cdk deploy --require-approval never --all --outputs-file out.json",
		"test:e2e": "jest --config ./jest.config.e2e.js --detectOpenHandles --runInBand",
		"test:e2e:watch": "jest --config ./jest.config.e2e.js --watch --detectOpenHandles --runInBand"
	},
	"devDependencies": {
		"@types/jest": "^29.5.13",
		"@types/node": "22.7.4",
		"aws-cdk": "^2.161.1",
		"aws-sdk": "^2.1691.0",
		"cdk-nag": "^2.28.196",
		"esbuild": "^0.24.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"typedoc": "^0.26.8",
		"typescript": "~5.6.2"
	},
	"dependencies": {
		"aws-cdk-lib": "2.161.1",
		"constructs": "^10.3.0",
		"source-map-support": "^0.5.21"
	}
}
